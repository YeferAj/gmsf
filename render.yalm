services:
  - type: web
    name: gmsf
    runtime: node                
    buildCommand: npm install    
    startCommand: npm start      
    env: node                    
    envVars:
      - key: DB_SSL
        value: true 
      - key: JWT_SECRET
        generateValue: true
      - key: NODE_ENV
        value: production
      - key: PORT
        value: 3000        

databases:
  - name: gmsf
    plan: free  
    databaseName: gmsf_p6s0  
    user: gym_admin      